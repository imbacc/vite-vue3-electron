{
	"name": "vite-vue3-electron",
	"version": "1.0.0",
	"license": "ISC",
	"author": "imbacc",
	"main": "./dist/electron/main.js",
	"scripts": {
		"dev": "vite --mode development",
		"dev:test": "vite --mode testing",
		"dev:prod": "vite --mode production",
		"dev:ms": "vite --debug transform",
		"force": "vite --mode development --force",
		"build": "rimraf ./deskApp && pnpm run v-build && cross-env NODE_ENV=production electron-builder",
		"divider1": "--------------------------------------------",
		"v-build": "rimraf ./dist/render && vite build --mode production",
		"v-build:test": "rimraf ./dist/render && vite build --mode testing",
		"v-build:prod": "rimraf ./dist/render && vite build --mode production",
		"v-preview:test": "vite --mode testing preview",
		"v-preview:prod": "vite --mode production preview",
		"divider2": "--------------------------------------------",
		"test:gzip": "npx http-server ./dist/render --cors --gzip -c-1",
		"lint": "eslint . --quiet",
		"lint:fix": "eslint . --fix --quiet",
		"czc": "git add . && cz",
		"preinstall": "npx only-allow pnpm",
		"prepare": "husky install",
		"up": "taze",
		"up:max": "taze major"
	},
	"build": {
		"appId": "vite.vue3.electron",
		"productName": "vite-vue3-electron",
		"files": [
			"dist/**/*",
			"public/**/*",
			"package.json"
		],
		"extraMetadata": {
			"author": "imbacc",
			"homepage": "https://github.com/imbacc/vite-vue3-electron.git",
			"description": "Electron application",
			"keywords": [
				"Electron"
			]
		},
		"directories": {
			"output": "deskApp"
		},
		"mac": {},
		"win": {
			"icon": "./dist/render/logo.png",
			"target": [
				"nsis"
			]
		},
		"nsis": {
			"oneClick": false,
			"language": "2052",
			"allowToChangeInstallationDirectory": true,
			"createDesktopShortcut": true
		},
		"publish": [
			{
				"provider": "generic",
				"url": "http://electron.com/download"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/imbacc/vite-vue3-electron.git"
	},
	"dependencies": {
		"axios": "^1.7.2",
		"dayjs": "^1.11.11",
		"electron-squirrel-startup": "^1.0.1",
		"imba-cache": "^1.1.13",
		"imba-request": "^1.0.25",
		"lodash-es": "^4.17.21",
		"mitt": "^3.0.1",
		"nprogress": "^0.2.0",
		"pinia": "^2.1.7",
		"qs-stringify": "^1.2.1",
		"vue": "^3.4.30",
		"vue-router": "^4.4.0"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^2.21.1",
		"@commitlint/config-conventional": "^19.2.2",
		"@types/bun": "^1.1.5",
		"@types/follow-redirects": "^1.14.4",
		"@types/lodash-es": "^4.17.12",
		"@unocss/preset-uno": "^0.57.7",
		"@unocss/vite": "^0.57.7",
		"@vitejs/plugin-vue": "^5.0.5",
		"@vitejs/plugin-vue-jsx": "^4.0.0",
		"@vue/compiler-sfc": "^3.4.30",
		"change-case": "^5.4.4",
		"commitizen": "^4.3.0",
		"commitlint": "^19.3.0",
		"cross-env": "^7.0.3",
		"cz-customizable": "^7.0.0",
		"electron": "^31.1.0",
		"electron-builder": "^24.13.3",
		"eslint-plugin-vue": "^9.26.0",
		"follow-redirects": "^1.15.6",
		"husky": "^9.0.11",
		"imba-config": "^2.0.2",
		"lint-staged": "^15.2.7",
		"mockjs": "^1.1.0",
		"nodemon": "^3.1.4",
		"rimraf": "^5.0.7",
		"rollup": "^4.18.0",
		"sass": "^1.69.7",
		"stylelint": "^16.6.1",
		"taze": "^0.13.8",
		"typescript": "^5.1.6",
		"unocss": "^0.57.7",
		"unplugin-auto-import": "^0.17.6",
		"unplugin-icons": "^0.19.0",
		"unplugin-vue-components": "^0.27.1",
		"vite": "^5.1.4",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-electron": "^0.28.7",
		"vite-plugin-electron-renderer": "^0.14.5",
		"vite-plugin-mock": "^3.0.2",
		"vite-plugin-pages": "^0.32.2",
		"vitest": "^1.6.0",
		"vue-tsc": "^2.0.22"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		}
	}
}
