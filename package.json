{
	"name": "vite2-vue3-electron",
	"version": "1.0.0",
	"license": "ISC",
	"author": "imbacc",
	"main": "./dist/electron/main.js",
	"scripts": {
		"dev": "concurrently -n=vue,electron -c=green,blue \"yarn run v-dev\" \"yarn run e-dev\"",
		"divider1": "--------------------------------------------",
		"v-dev": "vite --mode development --host",
		"v-dev:test": "vite --mode testing --host",
		"v-dev:prod": "vite --mode production --host",
		"v-build": "vite build --mode production",
		"v-build:test": "vite build --mode testing",
		"v-build:prod": "vite build --mode production",
		"v-preview:test": "vite --mode testing preview --host",
		"v-preview:prod": "vite --mode production preview --host",
		"v-force": "vite --mode development --host --force",
		"v-debug": "vite --mode development --host --debug",
		"divider2": "--------------------------------------------",
		"e-dev": "nodemon",
		"e-build64": "electron-forge make ./ winDist --platform=win64 --arch=x64  --out=./app --app-version=1.0.0 --overwrite --icon=./public/favicon.ico",
		"e-dist": "rm -rf ./dist && electron-forge package",
		"divider3": "--------------------------------------------",
		"typecheck": "vue-tsc --noEmit --skipLibCheck",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
		"lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
		"czc": "git add . && cz",
		"preinstall": "npx only-allow pnpm",
		"prepare": "husky install"
	},
	"build": {
		"appId": "douban-movie-electron",
		"files": [
			"package.json",
			"vite-plugin",
			"node_modules/**/*",
			"src/electron/**/*",
			"src/assets",
			"src/main.ts"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/imbacc/vite2-vue3-electron.git"
	},
	"dependencies": {
		"axios": "^1.3.4",
		"dayjs": "^1.11.7",
		"imba-cache": "^1.1.6",
		"imba-request": "^1.0.18",
		"lodash-es": "^4.17.21",
		"mitt": "^3.0.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.0.32",
		"qs-stringify": "^1.2.1",
		"vue": "^3.2.47",
		"vue-router": "^4.1.6"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^0.35.2",
		"@commitlint/config-conventional": "^17.4.4",
		"@electron-forge/cli": "^6.1.1",
		"@types/lodash-es": "^4.17.6",
		"@unocss/preset-uno": "^0.48.0",
		"@unocss/vite": "^0.48.0",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitejs/plugin-vue-jsx": "^3.0.0",
		"@vue/compiler-sfc": "^3.2.47",
		"change-case": "^4.1.2",
		"commitizen": "^4.3.0",
		"commitlint": "^17.4.4",
		"concurrently": "^8.0.1",
		"cross-env": "^7.0.3",
		"cz-customizable": "^7.0.0",
		"electron": "^24.1.2",
		"eslint": "^8.34.0",
		"eslint-plugin-vue": "^9.8.0",
		"husky": "^8.0.2",
		"imba-config": "^1.0.0",
		"lint-staged": "^13.1.0",
		"mockjs": "^1.1.0",
		"nodemon": "^2.0.22",
		"rollup": "^2.72.1",
		"sass": "^1.51.0",
		"stylelint": "^14.16.0",
		"typescript": "^4.5.4",
		"unocss": "^0.48.0",
		"unplugin-auto-import": "^0.12.1",
		"unplugin-icons": "^0.15.0",
		"unplugin-vue-components": "^0.22.12",
		"vite": "^4.1.4",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-mock": "^2.9.6",
		"vite-plugin-pages": "^0.28.0",
		"vitest": "^0.26.3",
		"vue-tsc": "^0.34.7"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		}
	}
}
